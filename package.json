{
  "name": "dev-story-scraper",
  "description": "Scraper to extract StackOverflow's Dev Story data",
  "license": "ISC",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "start:dev": "ts-node src/app.ts",
    "test": "SO_DEFAULT_START_DATE='2022-03-19' jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier 'src/**/*.{ts,js,json}' --write"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.10",
    "axios": "^0.26.0",
    "axios-retry": "^3.2.4",
    "cheerio": "^1.0.0-rc.10",
    "clean-deep": "^3.4.0",
    "express": "^4.17.3",
    "https-proxy-agent": "^5.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "pino": "^7.8.0",
    "socks-proxy-agent": "^6.2.0-beta.0",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/aws-lambda": "^8.10.92",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.178",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
